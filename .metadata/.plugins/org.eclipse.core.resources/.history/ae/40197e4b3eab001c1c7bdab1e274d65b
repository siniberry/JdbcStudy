package com.test;

public class MemberDTO
{
	// 주요 속성 구성
	private String empName, ssn, ibsaDate, cityName, tel, buseoName,jikwiName;
	private int empid, basicPay, sudang, salary;
	
	
	// getter / setter
	
	
	
	
	public void setSalary(int basicpay, int sudang)
	{
		this.salary = (basicpay * 12 ) + sudang;
	}

	
